--custom functions

function create_player()
	p={x=2,y=9,sprite=8}
end

function create_boss()
 b={x=72,y=03,sprite=84}
end

function player_movement()
	newx=p.x
	newy=p.y
	if(btnp(➡️)) newx+=1
	if(btnp(⬅️)) newx-=1
	if(btnp(⬇️)) newy+=1
	if(btnp(⬆️)) newy-=1
	
	interact(newx,newy)
	
	if not check_flag(0,newx,newy) then //si y'a pas d'obstacle then
		p.x=mid(0,newx,127)
		p.y=mid(0,newy,63)
	end
end


function check_flag(flag,x,y)
	local sprite=mget(x,y)
	return fget(sprite, flag)
end

function next_tile(x,y)
	sprite=mget(x,y)
	mset(x,y,sprite+1)
end

function lock(x,y)
	sprite=mget(x,y)
	mset(x,y,29)
	keys-=1
end

function pickup(x,y)
 next_tile(x,y)
 keys+=1
 passage-=1
 sfx(0)
 epee=1
end

function pickuppap(x,y)
 sfx(3)
end

function open_door(x,y)
	next_tile(x,y)
	keys-=1
 sfx(1)
end


function wait(t)
	for i = 1,t do
		flip()
	end
end

function tile(x,y)
	sprite=mget(x,y)
	mset(x,y,85)
end


