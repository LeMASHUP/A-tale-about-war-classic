--draw



function _draw()
	cls()
	draw_map()
	draw_player()
	draw_ui()
	draw_msg()
	draw_boss()
	if (btn(❎)) and epee==1 and not messages[1] then
 	draw_attack()
	end
	if mort==1 then
		_draw=draw_go
	end
	if fin==1 then
		_draw=draw_end
	end
end

function draw_player()
 spr(p.sprite, p.x*8, p.y*8)
 if (btnp(❎)) and epee==1 and not messages[1] then
 	p.sprite=86
 elseif not (btnp(❎)) then
 	p.sprite=8
 end
end

function draw_boss()
	spr(b.sprite, b.x*8, b.y*8)
end

function draw_ui()
	camera()
	palt(0,false)
	palt(11,true) --je change la couleur de transprance
	spr(9,2,2) --la cle de l'ui
	spr(15,2,10) --le sprite coeur de l'ui
	palt()
	spr(69,115,5)
	if epee ==1 then
		spr(70,115,5)
	end
	if quete==1 and not messages[1] then
		text_outline("allez au chateau",7,110)
	end
		if quete==2 and not messages[1] then
		text_outline("cherchez la cle",7,110)
	end
	if quete==3  and not messages[1] then
		text_outline("resoudre l'enigme",7,110)
	end
	if quete==4 and not messages[1] then
		text_outline("retournez au chateau",7,110)
	end
	if quete==5 and not messages[1] then
		text_outline("cherchez les cles de la salle",7,110)
	end
	if quete==6 and not messages[1] then
		text_outline("entrez dans les souterains",7,110)
	end
	text_outline("x"..keys,10,2)
	text_outline("x"..heart,10,10)
end

function draw_map()
	map(tilex, tiley, ecranx, ecrany,128,64)
end

function text_outline(text,x,y)
	print(text,x-1,y,0)
	print(text,x+1,y,0)
	print(text,x,y-1,0)
	print(text,x,y+1,0)
	print(text,x,y,7)
end

function draw_msg()
	if messages[1] then
		local y=100
		--personnage
		rectfill(7,y,11+#msg_title*4,y+7,5)
		print(msg_title,10,y+2,12)
		--dialogue
		rectfill(3,y+8,127,y+24,4)
		rect(3,y+8,127,y+24,15)
		print(messages[1],6,y+11,7)
	end
end

function draw_attack()
		p.sprite=86
	end
	
	
function draw_end()
	cls()
	print("a suivre")
end

function draw_go()
	cls()
	print("press ❎ to restart")
end


