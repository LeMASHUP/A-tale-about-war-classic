--update


function _update()
 
 if not messages[1] then
		player_movement()
	end
	if heart == 0 then
		mort=1
	end
	update_camera()
	update_msg()
	cheats()
		if mort==1 then
	  _update=update_go
		end
		if fin==1 then
			_update=update_end
		end
	end

function update_camera()
	camx=flr(p.x/16)*16
	camy=flr(p.y/16)*16
	camera(camx*8,camy*8)
end

function update_msg()
	if (btnp(❎)) then
		deli(messages,1)
	end
end



function update_end()
 if (btnp(❎)) then
  run()
 end
end

function update_go()
 if (btnp(❎)) then
  run()
 end
end

